[binaries]
c = 'riscv64-elf-gcc'

[host_machine]
system = 'riscv-toy-os'
cpu_family = 'riscv'
cpu = 'rv64'
endian = 'little'

[built-in options]
needs_exe_wrapper = true

c_args = '-march=rv64gc -mabi=lp64d -mcmodel=medany -ffreestanding'
c_link_args = '-march=rv64gc -mabi=lp64d -mcmodel=medany -nostdlib'
